<div class="panel panel-primary homepanel">
  <div class="panel-heading">
    <a href="/" class="btn btn-default signupmain btn-xs">Sign Up</a></br></br>
    <h3 class="panel-title">Login!</h3>
  </div>
  <div class="panel-body">
    <%= bootstrap_form_for :session, url: sessions_path do |f| %>
    <p>
    <%= f.text_field :email %>
    </p>
    <p>
    <%= f.password_field :password %>
    </p>
    <p>
    <%= f.submit :Login%>
    </p>
    <% end %>
  </div>
</div>